    "webpack-bundle-analyzer": "^2.8.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.25.1",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.5",
    "electron-debug": "^1.1.0",
    "electron-google-analytics": "^0.0.16",
    "electron-log": "^2.2.6",
    "electron-settings": "^3.1.3",
    "electron-updater": "^1.16.0",
    "history": "^4.6.1",
    "normalize.css": "^7.0.0",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-carousel": "^4.2.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-slick": "^0.14.11",
    "recharts": "^0.22.4",
    "redux": "^3.6.0",
    "redux-raven-middleware": "^1.2.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "slick-carousel": "^1.6.0",
    "source-map-support": "^0.4.15",
    "uuid": "^3.1.0"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=3.x",
    "yarn": ">=0.21.3"
  }
}
